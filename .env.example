# HemPa Mitra Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenAI Configuration (Required)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.7

# Application Configuration
LOG_LEVEL=INFO
SERVER_DEBUG=false

# Optional: Weather API Configuration
# Add weather API keys if using external weather services
WEATHER_API_KEY=your_weather_api_key

# Optional: Additional MCP Server Configuration
# Add API keys for other MCP servers you want to use
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token
BRAVE_API_KEY=your_brave_search_api_key
POSTGRES_CONNECTION_STRING=postgresql://user:pass@host:port/database

# Note: Never commit the actual .env file with real secrets!